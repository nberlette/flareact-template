{
  "name": "flareact-template",
  "version": "0.2.0",
  "license": "MIT",
  "description": "A boilerplate template for your next Flareact project",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "dev": "yarn lint && flareact dev",
    "build": "yarn lint:fix && flareact build",
    "deploy": "yarn lint:fix && flareact publish"
  },
  "dependencies": {
    "flareact": "1.3.0",
    "preact": "10.5.14",
    "preact-render-to-string": "5.1.19",
    "preact-ssr-prepass": "1.2.0",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-render-to-string": "npm:preact-render-to-string@5.1.19",
    "react-ssr-prepass": "npm:preact-ssr-prepass@1.2.0"
  },
  "repository": "github:nberlette/flareact-template",
  "author": {
    "name": "Nicholas Berlette",
    "email": "nick@berlette.com",
    "url": "https://nick.berlette.com/"
  },
  "bugs": "https://github.com/nberlette/flareact-template/issues",
  "homepage": "https://github.com/nberlette/flareact-template#readme",
  "devDependencies": {
    "standard": "~16.0.3",
    "eslint": "latest",
    "eslint-config-react": "latest",
    "eslint-config-prettier": "latest"
  },
  "keywords": [
    "flareact",
    "template",
    "cloudflare",
    "workers",
    "boilerplate"
  ],
  "readme": "https://github.com/nberlette/flareact-template#readme",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "semi": false
  }
}
