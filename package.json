{
  "name": "flareact-template-preact",
  "version": "0.2.0",
  "license": "MIT",
  "description": "Boilerplate for Flareact (Cloudflare Workers), but with Preact.",
  "main": "index.js",
  "scripts": {
    "install": "eslint --init && [ ! -e ./wrangler.toml ] && wrangler init",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix --ignore-pattern 'out/**' ./",
    "lint:dry": "eslint --fix-dry-run --ignore-pattern 'out/**' ./",
    "dev": "yarn lint && flareact dev",
    "build": "yarn lint:fix && flareact build",
    "deploy": "yarn build && flareact publish"
  },
  "dependencies": {
    "flareact": "1.3.0",
    "preact": "~10.5.14",
    "preact-render-to-string": "~5.1.19",
    "preact-ssr-prepass": "~1.2.0",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-render-to-string": "npm:preact-render-to-string",
    "react-ssr-prepass": "npm:preact-ssr-prepass"
  },
  "repository": "github:nberlette/flareact-template",
  "author": {
    "name": "Nicholas Berlette",
    "email": "nick@berlette.com",
    "url": "https://nick.berlette.com/"
  },
  "bugs": "https://github.com/nberlette/flareact-template/issues",
  "homepage": "https://github.com/nberlette/flareact-template#readme",
  "devDependencies": {
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-config-react": "~1.1.7",
    "eslint-plugin-react": "~7.24.0",
    "prettier": "~2.3.2"
  },
  "keywords": [
    "flareact",
    "template",
    "cloudflare",
    "workers",
    "boilerplate"
  ],
  "readme": "https://github.com/nberlette/flareact-template#readme",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "semi": false
  }
}
